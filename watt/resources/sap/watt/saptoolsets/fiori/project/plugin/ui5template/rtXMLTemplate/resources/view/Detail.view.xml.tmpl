<mvc:View xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form" xmlns:mvc="sap.ui.core.mvc" xmlns:template="http://schemas.sap.com/sapui5/extension/sap.ui.core.template/1">
	<VBox>
    	<template:with path="meta>entityType" helper="sap.ui.model.odata.AnnotationHelper.gotoEntityType" var="entityType">
    		<form:SimpleForm>
    			<template:if test="{entityType>com.sap.vocabularies.UI.v1.HeaderInfo}">
    				<template:with path="entityType>com.sap.vocabularies.UI.v1.HeaderInfo">
    				    <core:Title text="{path: 'entityType>TypeName', formatter: 'sap.ui.model.odata.AnnotationHelper.format'}"/>
    					<Label text="Type Name"/>
    					<Label text="{path: 'entityType>Title/Label', formatter: 'sap.ui.model.odata.AnnotationHelper.format'}"/>
    					<Text text="{path: 'entityType>Title/Value', formatter: 'sap.ui.model.odata.AnnotationHelper.format'}"/>
    					<template:if test="{entityType>Description}">
    						<Label text="{path: 'entityType>Description/Label', formatter: 'sap.ui.model.odata.AnnotationHelper.format'}"/>
    						<Text text="{path: 'entityType>Description/Value', formatter: 'sap.ui.model.odata.AnnotationHelper.format'}"/>
    					</template:if>
    				</template:with>
    			</template:if>
    			<template:if test="{entityType>com.sap.vocabularies.UI.v1.Identification}">
    				<template:repeat list="{entityType>com.sap.vocabularies.UI.v1.Identification}">
    					<Label text="{path: 'entityType>Label', formatter: 'sap.ui.model.odata.AnnotationHelper.format'}"/>
    					<Text text="{path: 'entityType>Value', formatter: 'sap.ui.model.odata.AnnotationHelper.format'}"/>
    				</template:repeat>
    			</template:if>
    		</form:SimpleForm>
    
        <!-- <form:SimpleForm>-->
        <!--  <form:title>-->
        <!--    <core:Title text="Facets"/>-->
        <!--  </form:title>-->
        <!--  <template:with path="entityType>com.sap.vocabularies.UI.v1.Facets" var="facets">-->
        <!--    <core:Fragment fragmentName="{{rtXMLTemplate.parameters.ProjectNamespace.value}}.view.Facets" type="XML"/>-->
        <!--  </template:with>-->
        <!--</form:SimpleForm>-->
        </template:with>
    </VBox>
</mvc:View>