{
    "name": "sap.hana.cst.common.sqlcodecompletion",
    "description": "The SqlCodeCompletion Plugin",
    "i18n" : "sap.hana.cst.common.sqlcodecompletion/i18n/i18n.hdbtextbundle",

    "requires": {
        "services": [
            "selection" ,
            "xs2CtrlDAO"
        ]
    },

    "provides": {
        "services": {
            "sqlcodecompletion": {
                "implements": "sap.hana.cst.common.sqlcodecompletion.interface.SqlCodeCompletion",
                "module": "sap.hana.cst.common.sqlcodecompletion/service/SqlCodeCompletionImpl"
            },
            "sqlcodecompletionDAO": {
                "implements": "sap.hana.cst.common.sqlcodecompletion.interface.SqlCodeCompletionDAO",
                "module": "sap.hana.cst.common.sqlcodecompletion/service/SqlCodeCompletionDAO"
            }
        },
        "interfaces": {
            "sap.hana.cst.common.sqlcodecompletion.interface.SqlCodeCompletion": "sap.hana.cst.common.sqlcodecompletion/service/SqlCodeCompletion",
            "sap.hana.cst.common.sqlcodecompletion.interface.SqlCodeCompletionDAO": "sap.hana.cst.common.sqlcodecompletion/service/SqlCodeCompletionDAO",
            "sap.hana.cst.common.sqlcodecompletion.interface.SqlSchemaProvider": "sap.hana.cst.common.sqlcodecompletion/service/SqlSchemaProvider"
        }
    },

    "configures": {
        "services": {
            "sqlcodecompletion:libraries": [{
                "name": "ui5",
                "version": "1.18.6"
            }]
        }
    },

    "subscribes": {
        "selection:changed": "sqlcodecompletion:onAfterSetSelection"
    }

}