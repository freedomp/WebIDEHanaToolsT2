{
	"name" : "sap.watt.common.service.ui.Console",
	"description" : "The console interface",
	"extends" : "sap.watt.common.service.ui.Part",

	"configurationProperties" : {
		"targets" : {
			"type" : "string",
			"multiple" : true
		},
		"maxLineCount" : {
			"type" : "number",
			"multiple" : false
		},
		"format" : {
			"type" : "string",
			"multiple" : false
		}
	},

	"methods" : {

		"addMessage" : {
			"params" : [{
				"name" : "sMessage",
				"type" : {"timestamp": "object", "tag": "string", "level": "string", "message": "string"}
			}]
		}, 
		"clear" : {}
	}
}