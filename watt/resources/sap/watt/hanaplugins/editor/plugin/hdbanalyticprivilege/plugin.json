{
    "name": "sap.watt.hanaplugins.editor.plugin.hdbanalyticprivilege",
    "description": "analytic privilege editor",
    "i18n": "sap.watt.hanaplugins.editor.plugin.hdbanalyticprivilege/i18n/messageBundle.hdbtextbundle",

    "requires": {
        "services": [
            "content",
            "selection",
            "editor",
            "document",
            "resource",
            "command",
            "commandGroup",
            "aceeditor",
            "repositorybrowser",
            "analyticprivilegeeditor",
            "repositorybrowserUtils",
            "chebackend.XS2ServicesDAO"
            
        ]
    },
    "provides": {
        "services": {
            "analyticprivilegeeditor": {
                "implements": "sap.watt.hanaplugins.editor.plugin.hdbanalyticprivilege.interface.AnalyticPrivilegeEditorContainer",
                "module": "sap.watt.hanaplugins.editor.plugin.hdbanalyticprivilege/service/AnalyticPrivilegeEditorContainer"
            }
        },
        "interfaces": {
            
            "sap.watt.hanaplugins.editor.plugin.hdbanalyticprivilege.interface.AnalyticPrivilegeEditorContainer": "sap.watt.hanaplugins.editor.plugin.hdbanalyticprivilege/service/AnalyticPrivilegeEditorContainer"
        }
    },
    "configures": {

        "services": {
            "analyticprivilegeeditor:styles": [{
                "uri": "sap.watt.hanaplugins.editor.plugin.hdbanalyticprivilege/css/analyticprivilege.css"
            }],
            "editor:editors": [{
                "fileExtension": [
                    "hdbanalyticprivilege", "HDBANALYTICPRIVILEGE"
                ],
                "service": "@analyticprivilegeeditor",
                "name": "{i18n>tit_open_analytic_privilege}",
                "id": "analyticprivilegeeditor"
            }, {
                "fileExtension": [
                    "hdbanalyticprivilege", "HDBANALYTICPRIVILEGE"
                ],
                "service": "@aceeditor",
                "name": "{i18n>tit_open_analytic_privilege_text}",
                "id": "analyticprivilegeeditor_ace"
            }],

            "editor:defaultEditors": [{
                "extention": "hdbanalyticprivilege",
                "editorId": "analyticprivilegeeditor"
            }],

            "command:commands": [{
                "id": "analyticprivilegeeditor.create",
                "service": "sap.watt.hanaplugins.editor.plugin.hdbanalyticprivilege/commands/NewAnalyticPrivilegeFile",
                "label": "{i18n>tit_create_analytic_privilege}"
            },{
                "id": "analyticprivilegeeditor.undo",
                "service": "sap.watt.hanaplugins.editor.plugin.hdbanalyticprivilege/commands/Undo",
                "label": "{i18n>txt_undo}"
            },{
                "id": "analyticprivilegeeditor.redo",
                "service": "sap.watt.hanaplugins.editor.plugin.hdbanalyticprivilege/commands/Redo",
                "label": "{i18n>txt_redo}"
            }],

            "commandGroup:items": [{
                "parent": "file.new",
                "command": "analyticprivilegeeditor.create",
                "prio": 4
            }]

        }
    }
}