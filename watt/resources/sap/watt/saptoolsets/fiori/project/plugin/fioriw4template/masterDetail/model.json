{

    "datasource" : 
    {
        "type" : "odata",
        "url" : ""
    },

    "masterDetail" :
    {
        "parameters" : 
        {
           "ProjectNamespace" :
                {
                    "type" : "string",
                    "value" : "",
                    "wizard" : 
                    {
                        "control" : "TextField",
                        "required" : true,
                        "title" : "Project Namespace"
                    }
            },
            "MasterTitle" :
                {
                    "type" : "string",
                    "value" : "",
                    "wizard" : 
                    {
                        "control" : "TextField",
                        "required" : false,
                        "title" : "Master List Title"
                    }
                },
            "Collection1" :
                {
                    "type" : "Entity",
                    "multiplicity" : "many",
                	"isRoot" : true,
                	"binding" : "@datasource.entities",
                    "value" : "",
                    "wizard" : 
                    {
                        "control" : "ComboBox",
                        "required" : true,
                        "title" : "ML Collection"
                    }
            },
            "MavenProjectName" :
            {
                    "type" : "string",
                    "value" : "@masterDetail.parameters.ProjectNamespace.value",
                    "wizard" :
                    {
                        "control" : "TextField",
                        "required" : true,
                        "title" : "Name"
                    }
            },
            "MavenProjectDescription" :
            {
                    "type" : "string",
                    "value" : "",
                    "wizard" :
                    {
                        "control" : "TextField",
                        "required" : true,
                        "title" : "Description"
                    }
            },
            "MavenProjectArtifactId" :
            {
                    "type" : "string",
                    "value" : "@masterDetail.parameters.ProjectNamespace.value",
                    "wizard" :
                    {
                        "control" : "TextField",
                        "required" : true,
                        "title" : "Artifact Id"
                    }
            },
            "MavenProjectParentVersion" :
            {
                    "type" : "string",
                    "value" : "1.6.0-SNAPSHOT",
                    "wizard" :
                    {
                        "control" : "TextField",
                        "required" : true,
                        "title" : "Parent Version"
                    }
            },
            "Title" :
                {
                    "type" : "string",
                    "multiplicity" : "one",
                    "binding" : "@masterDetail.parameters.Collection1.value.elements",
                    "value" : "",
                    "wizard" : 
                    {
                        "control" : "ComboBox",
                        "required" : true,
                        "title" : "List Item (LI) Title"
                    }
            },
            "Number" :
                {
                    "type" : "string",
                    "multiplicity" : "one",
                    "binding" : "@masterDetail.parameters.Collection1.value.elements",
                    "value" : "",
                    "wizard" : 
                    {
                        "control" : "ComboBox",
                        "required": false,
                        "title" : "LI Number: Value"
                    }
            },
            "NumberUnit" :
                {
                    "type" : "string",
                    "multiplicity" : "one",
                    "binding" : "@masterDetail.parameters.Collection1.value.elements",
                    "value" : "",
                    "wizard" : 
                    {
                        "control" : "ComboBox",
                        "required": false,
                        "title" : "LI Number: Unit"
                    }
            },
            "Attribute1" :
                {
                    "type" : "string",
                    "multiplicity" : "one",
                    "binding" : "@masterDetail.parameters.Collection1.value.elements",
                    "value" : "",
                    "wizard" : 
                    {
                        "control" : "ComboBox",
                        "required": false,
                        "title" : "LI Attribute 1"
                    }
            },
            "Attribute2" :
                {
                    "type" : "string",
                    "multiplicity" : "one",
                    "binding" : "@masterDetail.parameters.Collection1.value.elements",
                    "value" : "",
                    "wizard" : 
                    {
                        "control" : "ComboBox",
                        "required": false,
                        "title" : "LI Attribute 2"
                    }
            },
            "ServiceDetails" :
            {
                 "serviceName" : ""
            }
        },
        "forms" : [
            {
                "title" : "Master Detail App",
                "groups" : [
                    {
                        "title" : "Project Settings",
                        "parameters": ["@masterDetail.parameters.ProjectNamespace", "@masterDetail.parameters.MasterTitle"]
                    },
                    {
                        "title" : "Master List (ML) Item Settings",
                        "parameters": ["@masterDetail.parameters.Collection1","@masterDetail.parameters.Title","@masterDetail.parameters.Number","@masterDetail.parameters.NumberUnit","@masterDetail.parameters.Attribute1","@masterDetail.parameters.Attribute2"]
                    },
                    {
                        "title" : "Maven Project Settings",
                        "parameters": ["@masterDetail.parameters.MavenProjectName","@masterDetail.parameters.MavenProjectDescription","@masterDetail.parameters.MavenProjectArtifactId","@masterDetail.parameters.MavenProjectParentVersion"]
                    }
                ]
            }
        ]
    }
}