{
	"name": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor",
	"description": "The UI5 Wysiwyg Editor Plugin",
	"i18n": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/i18n/i18n",
	"requires": {
		"services": [
			"outlinePane",
			"editor",
			"document",
			"usernotification",
			"resource",
			"focus",
			"log",
			"command",
			"content",
			"selection",
			"perspective",
			"commandGroup",
			"contextMenu",
			"ui5projecthandler",
			"setting.project",
			"filesystem.documentProvider",
			"odataProvider",
			"metadataHandler",
			"preferences",
			"ui5icons",
			"dialogprogress",
			"progress",
			"usagemonitoring",
			"intellisence",
			"intellisence.libmetadataprovider",
			"template",
			"generation",
			"projectType",
			"repositorybrowser",
			"tipsandtricks",
			"tipsandtricksviewsfactory"
		]
	},
	"provides": {
		"services": {
			"ui5wysiwygeditor": {
				"implements": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor.WYSIWYGEditor",
				"module": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/service/WysiwygEditor"
			},
			"w5gproperties": {
				"implements": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor.W5gProperties",
				"module": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/service/W5gProperties"
			},
			"w5gOutline": {
				"implements": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor.W5GOutline",
				"module": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/service/W5GOutline"
			},
			"w5gEmptyOutline": {
				"implements": "sap.watt.common.outlinepane.Outline",
				"module": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/service/W5GEmptyOutline"
			},
			"quickstart": {
				"implements": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor.QuickStartService",
				"module": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/service/QuickStartService"
			}
		},
		"interfaces": {
			"sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor.WYSIWYGEditor": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/service/WysiwygEditor",
			"sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor.W5gProperties": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/service/W5gProperties",
			"sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor.W5GOutline": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/service/W5GOutline",
			"sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor.QuickStartService": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/service/QuickStartService"
		}
	},
	"configures": {
		"services": {
			"ui5wysiwygeditor:styles": [
				{
					"uri": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/styles/ui5wysiwygeditor.css"
				}
			],
			"editor:editors": [
				{
					"fileExtension": [
						"view.xml",
						"fragment.xml"
					],
					"service": "@ui5wysiwygeditor",
					"name": "Layout Editor",
					"id": "ui5wysiwygeditor"
				}
			],
			"template:categories" : [
				{
					"id" : "LayoutEditor.Sample",
					"name" : "Layout Editor sample applications",
					"description" : "Layout Editor sample applications"
				}
			],
			"template:templates" : [
				{
					"id" : "wysiwygtemplate.quickStart",
					"template" : "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/templates/quickStartTemplate/quickStartTemplate",
					"name" : "Quick Start Template",
					"description" : "Template for quick start with Layout Editor",
					"path" : "ui5wysiwygeditor/templates/quickStartTemplate",
					"fileName" : "quickStartTemplate.zip",
					"category" : "LayoutEditor.Sample",
					"templateType" : "quickStartTemplate",
					"requiresNeoApp" : true
				}
			],
			"ui5wysiwygeditor:contextMenu": "ui5wysiwygeditorContextMenu",
			"w5gOutline:contextMenu": "w5gOutlineContextMenu",
			"selection:typedSelections": [
				{
					"id": "w5gOutline",
					"service": "@w5gOutline"
				}
			],
			"w5gOutline:styles": [
				{
					"uri": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/styles/w5goutline.css"
				}
			],
			"outlinePane:outlines": [
				{
					"editor": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor.WYSIWYGEditor",
					"service": "@w5gEmptyOutline"
				}
			],
			"command:commands": [
				{
					"id": "ui5wysiwygeditor.control.remove",
					"label": "{i18n>w5g_editor_command_remove}",
					"type": "action",
					"icon": "remove",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/command/Remove",
					"keyBinding": "del"
				},
				{
					"id": "ui5wysiwygeditor.control.toggle",
					"label": "{i18n>w5g_editor_command_deselect}",
					"type": "action",
					"icon": "revert",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/command/Toggle",
					"keyBinding": "esc"
				},
				{
					"id": "ui5wysiwygeditor.control.slctpresibling",
					"label": "{i18n>w5g_editor_command_select_prev_sibling}",
					"type": "action",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/command/GoPreviousSibling",
					"keyBinding": "left"
				},
				{
					"id": "ui5wysiwygeditor.control.slctnextsibling",
					"label": "{i18n>w5g_editor_command_select_next_sibling}",
					"type": "action",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/command/GoNextSibling",
					"keyBinding": "right"
				},
				{
					"id": "ui5wysiwygeditor.control.slctparent",
					"label": "{i18n>w5g_editor_command_select_parent}",
					"type": "action",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/command/GoParent",
					"keyBinding": "up"
				},
				{
					"id": "ui5wysiwygeditor.control.slctchild",
					"label": "{i18n>w5g_editor_command_select_child}",
					"type": "action",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/command/GoChild",
					"keyBinding": "down"
				},
				{
					"id": "ui5wysiwygeditor.control.gotocode",
					"label": "{i18n>w5g_editor_command_goto_code}",
					"type": "action",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/command/GoToCode",
					"keyBinding": "ctrl+f3"
				},
				{
					"id": "ui5wysiwygeditor.openFragmentInLayoutEditor",
					"label": "{i18n>w5g_editor_command_open_fragment_in_layout_editor}",
					"type": "action",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/command/OpenFragmentInLayoutEditor"
				},
				{
					"id": "ui5wysiwygeditor.openSubviewInLayoutEditor",
					"label": "{i18n>w5g_editor_command_open_subview_in_layout_editor}",
					"type": "action",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/command/OpenSubviewInLayoutEditor"
				},
				{
					"id": "ui5wysiwygeditor.control.cut",
					"label": "{i18n>w5g_editor_command_cut}",
					"type": "action",
					"keyBinding": "ctrl+x",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/command/Cut"
				},
				{
					"id": "ui5wysiwygeditor.control.copy",
					"label": "{i18n>w5g_editor_command_copy}",
					"type": "action",
					"keyBinding": "ctrl+c",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/command/Copy"
				},
				{
					"id": "ui5wysiwygeditor.control.paste",
					"label": "{i18n>w5g_editor_command_paste}",
					"type": "action",
					"keyBinding": "ctrl+v",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/command/Paste"
				},
				{
					"id": "ui5wysiwygeditor.control.pasteBefore",
					"label": "{i18n>w5g_editor_command_paste_before}",
					"type": "action",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/command/PasteBefore"
				},
				{
					"id": "ui5wysiwygeditor.control.pasteAfter",
					"label": "{i18n>w5g_editor_command_paste_after}",
					"type": "action",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/command/PasteAfter"
				},
				{
					"id": "ui5wysiwygeditor.control.add",
					"label": "{i18n>w5g_editor_command_add}",
					"type": "action",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/command/Add"
				},
				{
					"id": "ui5wysiwygeditor.quickStart",
					"label": "{i18n>w5g_editor_command_quickStart}",
					"type": "action",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/command/QuickStartWithLayoutEditor",
					"available"  : {"not" : {"equals" : [{"env":"server_type"},"xs2"]}}
				}
			],
			"commandGroup:groups": [
				{"id": "ui5wysiwygeditorContextMenu"},
				{"id": "w5gOutlineContextMenu"},
				{"id": "w5g.new"},
				{"id": "w5g.open"},
				{"id": "w5g.actions"},
				{"id": "w5g.delete"}
			],
			"commandGroup:items": [
				{
					"parent":"edit.fileActions",
					"command": "ui5wysiwygeditor.control.cut",
					"prio" : 0
				},
				{
					"parent":"edit.fileActions",
					"command": "ui5wysiwygeditor.control.copy",
					"prio" : 1
				},
				{
					"parent":"edit.fileActions",
					"command": "ui5wysiwygeditor.control.paste",
					"prio" : 2
				},
				{
					"parent":"edit.fileActionDelete",
					"command": "ui5wysiwygeditor.control.remove",
					"prio" : 4
				},
				{
					"parent":"edit.gotodefinition",
					"command": "ui5wysiwygeditor.control.gotocode",
					"prio" : 5
				},
				{
					"parent": "w5gOutlineContextMenu",
					"type": "inline",
					"group": "w5g.new",
					"prio": 0
				},
				{
					"parent": "w5gOutlineContextMenu",
					"type": "inline",
					"group": "w5g.actions",
					"prio": 1
				},
				{
					"parent": "w5gOutlineContextMenu",
					"type": "inline",
					"group": "w5g.delete",
					"prio": 2
				},
				{
					"parent": "ui5wysiwygeditorContextMenu",
					"type": "inline",
					"group": "w5g.open",
					"prio": 1
				},
				{
					"parent": "ui5wysiwygeditorContextMenu",
					"type": "inline",
					"group": "w5g.actions",
					"prio": 2
				},
				{
					"parent": "ui5wysiwygeditorContextMenu",
					"type": "inline",
					"group": "w5g.delete",
					"prio": 3
				},
				{
					"parent": "w5g.new",
					"command": "ui5wysiwygeditor.control.add",
					"prio": 10
				},
				{
					"parent": "w5g.open",
					"command": "ui5wysiwygeditor.control.gotocode",
					"prio": 10
				},
				{
					"parent": "w5g.open",
					"command": "ui5wysiwygeditor.openFragmentInLayoutEditor",
					"prio": 20
				},
				{
					"parent": "w5g.open",
					"command": "ui5wysiwygeditor.openSubviewInLayoutEditor",
					"prio": 20
				},
				{
					"parent": "w5g.actions",
					"command": "ui5wysiwygeditor.control.cut",
					"prio": 40
				},
				{
					"parent": "w5g.actions",
					"command": "ui5wysiwygeditor.control.copy",
					"prio": 50
				},
				{
					"parent": "w5g.actions",
					"command": "ui5wysiwygeditor.control.paste",
					"prio": 60
				},
				{
					"parent": "w5g.actions",
					"command": "ui5wysiwygeditor.control.pasteBefore",
					"prio": 70
				},
				{
					"parent": "w5g.actions",
					"command": "ui5wysiwygeditor.control.pasteAfter",
					"prio": 80
				},
				{
					"parent": "w5g.delete",
					"command": "ui5wysiwygeditor.control.remove",
					"prio": 100
				},
				{
					"parent": "file.new.template",
					"command": "ui5wysiwygeditor.quickStart",
					"prio": 100
				}
			],
			"usagemonitoring:reportParams": [
				{
					"components": [
						"w5g"
					],
					"eventTypes": [
						"open_w5g",
						"palette_drag",
						"delete_control",
						"data_binding_OK",
						"data_binding_CANCEL",
						"delete_confirm_OK",
						"delete_confirm_CANCEL",
						"breadcrumb",
						"property_change",
						"outline_press",
						"outline_control_added"
					]
				}
			],
			"tipsandtricks:tips": [
				{
					"id": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor.OpenLayoutEditor",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/tips/OpenLayoutEditorTip"
				},
				{
					"id": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor.MessageToolbar",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/tips/MessageToolbarTip"
				},
				{
					"id": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor.AddControlsFromOutline",
					"service": "sap.watt.saptoolsets.fiori.editor.ui5wysiwygeditor/tips/AddControlsFromOutlineTip"
				}
			]
		}
	},
	"subscribes": {
		"ui5wysiwygeditor:viewHasChanged": [
			"w5gOutline:onViewHasChanged",
			"w5gproperties:onViewHasChanged"
		],
		"ui5wysiwygeditor:bindingChanged": "ui5wysiwygeditor:onDataBindingChanged",
		"ui5wysiwygeditor:viewElementSelected": [
			"ui5wysiwygeditor:onViewElementSelected",
			"w5gOutline:onViewElementSelected",
			"w5gproperties:onViewElementSelected"
		],
		"document:changed": "ui5wysiwygeditor:onDocumentChanged",
		"metadataHandler:metadataUploaded": "quickstart:onMetadataUploaded"
	}
}