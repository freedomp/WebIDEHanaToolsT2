{
	"name": "sap.watt.ideplatform.chewebapprunner",
	"description": "Che Web App runner plugin",
	"i18n" : "sap.watt.ideplatform.chewebapprunner/i18n/i18n",

	"requires": {
		"services": [
		    "run",
			"filesystem.documentProvider",
			"selection",
			"appparams",
			"usernotification",
			"baserunconfigui",
			"setting.project",
			"projectsetting",
			"basefilesearchutil",
			"basechoosefilepopup",
			"baserunnerrunutil",
			"baseinputvalidatorutil",
			"appmetadata",
			"log",
			"runconfig.filepath",
			"runconfig.preview",
			"runconfig.urlparameters"	
		]
	},
	"provides": {
		"services": {
			"CheWebAppRunner": {
				"implements": "sap.watt.common.plugin.run.service.runner",
				"module": "sap.watt.ideplatform.chewebapprunner/service/CheWebAppRunner"
			}
		},
		"interfaces": {
		
		}	
	},
	
 	"configures" : {
        "services" : {
            "run:runners": [
                {
                    "id" : "chewebapprunner",
                    "service": "@CheWebAppRunner",
                    "displayName" : "{i18n>lbl_run_sap_web_app}",
                    "projectTypesIds" : ["sap.html5"],
                    "windowTarget" : "web_preview",
					"fileTypes" : ["*.html"],
					"registerRunConsole": true
                }
            ]
        }
	},
	"subscribes": {
	
	}
}