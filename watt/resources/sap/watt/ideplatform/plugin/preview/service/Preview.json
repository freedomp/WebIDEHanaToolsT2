{
	"name" : "sap.watt.common.plugin.preview.service.Preview",
	"description" : "The preview interface",
	"methods" : {
		"showPreview" : {
			"params" : [{
				"name" : "oDocument",
				"type" : "object"
			},{
				"name" : "oWindow",
				"type" : "object",
				"description" : "HTMLIFrameElement"
			},{
				"name" : "bNoFrame",
				"type" : "boolean"
			},{
				"name" : "oCustomSettings",
				"type" : "object"
			},{
				"name" : "oRunConfiguration",
				"optional" : true,
				"type" : "object"
            }]
		},

		"getPreviewUrl" : {
			"params" : [{
				"name" : "oDocument",
				"type" : "object"
			},{
				"name" : "sMode",
				"type" : "string",
				"optional" : true,
				"description" : "The preview mode to apply"
			},{
				"name" : "oAppForwarding",
				"type" : "object",
				"optional" : true,
				"description" : "Application resource mapping object"
			},{
				"name" : "sUI5Version",
				"type" : "string",
				"optional" : true,
				"description" : "UI5 Version to be used"
			},{
				"name" : "bEmbeddedMode",
				"type" : "boolean",
				"optional" : true,
				"description" : "Is running using embedded runner"
			},{
				"name" : "sRelativePath",
				"type" : "string",
				"optional" : true,
				"description" : "Relative Path"
			},{
				"name" : "sSwitchBE",
				"type" : "string",
				"optional" : true,
				"description" : "the mapped backend systems"
			}],
			"returns" : {
				"type" : "object",
				"description" : "The URI object"
			}
		},
		"isExecutable" : {
			"params" : [{
				"name" : "oSelection",
				"type" : "object"
			}]
		}
	},
	
	"configurationProperties" : {
	    "cheadapter": {
	      "type" : {
	    	"service" : "sap.watt.common.plugin.preview.service.ChePreviewAdapter"
	      },
	      "multiple" : false
	    },
		"adapter" : {
			"type" : {
				"service" : "sap.watt.common.plugin.preview.service.PreviewAdapter"
			},
			"multiple" : false
		},
		"modes" : {
			"type" : {
				"id" : "string",
				"service" : "sap.watt.common.plugin.preview.service.PreviewMode"	
			},
			"multiple" : true
		},
		"runner" : {
			"type" : {
				"id" : "string",
				"service" : "sap.watt.common.plugin.preview.service.PreviewRunner"	
			},
			"multiple" : true
		},
		"preprocessors" : {
			"type" : {
				"id" : "string",
				"service" : "sap.watt.common.plugin.preview.service.PreviewPreProcessor"	
			},
			"multiple" : true
		}
	},
	
    "events" : {
        "buttonClicked" : {
        	"params" : [
                {
                    "name" : "id",
                    "type" : "string",
                    "description" : "ID of the button clicked on"
                }
            ]
        }
    }
}