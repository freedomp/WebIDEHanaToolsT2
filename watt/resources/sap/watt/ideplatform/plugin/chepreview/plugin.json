{
	"name": "sap.watt.ideplatform.chepreview",
	
	"description": "The Che Backend Plugin",
	"i18n" : "sap.watt.ideplatform.chepreview/i18n/i18n",
	
	"requires": {
		"services": [
			"document",
			"filesystem.documentProvider",
			"log",
			"preferences",
			"preview",
			"setting.project",
			"runRegistry",
			"usernotification"
		]
	},
	
	"provides": {
		"services": {
			"chePreviewAdapter": {
				"implements": "sap.watt.common.plugin.preview.service.ChePreviewAdapter",
				"module": "sap.watt.ideplatform.chepreview/service/ChePreviewAdapter"
			}
		}
		
	},
	
	"configures": {
		"services": {
			"preview:cheadapter":{
				"service": "@chePreviewAdapter"
			}
		}
	}
}