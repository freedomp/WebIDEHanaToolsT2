{
	"name" : "sap.watt.common.service.ui.Browser",
	"description" : "The browser interface",
	"extends" : ["sap.watt.common.service.ui.Part", "sap.watt.common.service.selection.Provider"],
	"configurationProperties" : {
		"contextMenu" : {
			"type" : "string",
			"multiple" : false
		},
		"decorationEnabled" : {
			"type" : "boolean",
			"multiple" : false
		},        
		"doubleClickEnabled" : {
			"type" : "boolean",
			"multiple" : false
		},  
		"multipleSelectionEnabled" : {
			"type" : "boolean",
			"multiple" : false
		},      
        "preferenceEnabled" : {
            "type" : "boolean",
            "multiple" : false
        },      
        "filters" : {
            "type" : "[string]",
            "multiple" : true
        }
		
		
	},
	"methods" : {
	
		"toggleLinkWithEditor" : { 
   			"params" : [] 
   		},
   		"getStateLinkWithEditor" : { 
   			"params" : []
   		},  
   		"setSelectionToCurrentDocument" : {
   			"params" : []
   		},	
		"setNodeCustomStyle" : {
			"params" : [{
				"name" : "oCustomStyle",
				"type" : "object"
			}]
		},
		"removeNodeCustomStyle" : {
			"params" : []
		},
		
		"setSelection" : {
			"params" : [{
				"name" : "oDocument",
				"type" : "object",
				"description": "document to be selected"
			},{
				"name" : "bExpand",
				"type" : "boolean",
				"description": "expands browser tree till the provided oDocument and does not expands the document itself"
			},
			{
				"name" : "bIncludingMyself",
				"type" : "boolean",
                          	"optional" : true,
				"description": "also expands the provided oDocument"
			}]
		},
		
		"refresh" : {
			"params" : []
		},
		"updateDecoration" : {
			"params" : [{
				"name" : "oDocument",
				"type" : "object"
			}, {
                "name" : "oEvent",
                "type" : "object",
                "optional" : true,
                "description" : "The event which triggered the decoration update"
            }]
		},
		"refreshDecorationsForAllChildren" : {
			"params" : [{
				"name" : "oDocument",
				"type" : "object"
			}, {
                "name" : "oEvent",
                "type" : "object",
                "optional" : true,
                "description" : "The event which triggered the decoration update"
            }]
		},
		"exportSelection" : {
			"params" : [{
				"name" : "aSelection",
				"type" : "object"
			},{
				"name" : "fileName",
				"type" : "string"
			}]
		}
	}
}