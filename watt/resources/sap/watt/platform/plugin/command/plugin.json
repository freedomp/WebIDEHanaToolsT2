{
	"name": "sap.watt.platform.command",
	
	"description": "The Command Plugin",
	
	"requires": {
		"services": [
			"focus"
		]
	},

	"provides": {
	
		"services" : {
			"command": {
				"implements": "sap.watt.common.service.registry.Command",
				"module" : "sap.watt.platform.command/service/Command"
			}
		}
		
	},
	
	"subscribes": {
		"focus:$dialogOpened": "command:onDialogOpened",
		"focus:$dialogClosed": "command:onDialogClosed"
	}
}
