{
	"name" : "sap.watt.common.service.ide.LibraryMetadataProvider",
	"description": "The library metadata provider interface",
	"methods" : {
		"getVersions" : {
			"params" : [
				{
					"name" : "sLibraryName",
					"type" : "string"
				}
			],
		   "returns" : { 
        		"type" : ["string"],
                "description" : "supported library versions"
            }
		},
		"getMetadata" : {
			"params" : [
				{
					"name" : "sLibraryName",
					"type" : "string"
				},
				{
					"name" : "sLibraryType",
					"type" : "string"
				},
				{
					"name" : "sVersion",
					"type" : "string"
				}
			],
		   "returns" : { 
        		"type" : {
        			"metadata" : "object",
        			"templates" : "object"
        		},
                "description" : "library metadata object of specific version. metadata and templates should be zip objects"
            }
		}
	}
}