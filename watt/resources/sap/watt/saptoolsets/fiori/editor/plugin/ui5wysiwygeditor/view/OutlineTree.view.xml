<core:View height="100%" controllerName="sap.watt.saptoolsets.fiori.editor.plugin.ui5wysiwygeditor.view.OutlineTree"
           xmlns="sap.watt.saptoolsets.fiori.editor.plugin.ui5wysiwygeditor.view" xmlns:core="sap.ui.core" xmlns:commons="sap.ui.commons">
    <commons:Toolbar>
        <commons:rightItems>
            <commons:Button icon="sap-icon://add" press="onAdd"
                            enabled="{toolbarModel>/isAddable}"
                            tooltip="{i18n>w5g_outline_add_control_tooltip}"/>
            <commons:Button icon="sap-icon://delete" press="onDelete"
                            enabled="{toolbarModel>/isDeletable}"
                            tooltip="{i18n>w5g_outline_delete_control_tooltip}"/>
        </commons:rightItems>
    </commons:Toolbar>
    <OutlineTree id="outlineTree" showHeader="false" showHorizontalScrollbar="true" class="sapWattOutlineTree">
    </OutlineTree>

</core:View>