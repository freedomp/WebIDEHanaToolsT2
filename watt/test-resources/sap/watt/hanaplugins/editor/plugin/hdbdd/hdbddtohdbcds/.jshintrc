{ // Configuration for "JSHint" checker, see http://www.jshint.com/docs/
    // -- enforcing options (a.k.a. positive list of checks)
    "curly": true,
    // "eqeqeq":true,
    "forin": true,
    "immed": true,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "noempty": true,
    "nonew": true,
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": true,
    // -- relaxing options (a.k.a negative list of checks)
    "eqnull": true,
    // -- globals tolerated in _all_ files
    "globals": {
    "define": false,
        "require": false,
        "assert": false
}
}
