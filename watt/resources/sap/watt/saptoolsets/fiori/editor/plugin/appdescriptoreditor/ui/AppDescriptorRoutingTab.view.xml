												<layout:GridData linebreak="true" span="L3 M3 S6"/>
											</layoutData>
										</Label>
									</form:label>
									<form:fields>
										<DropdownBox name="targetsTransition" change="onRoutingTargetsTransitionChange" enabled="{/manageTargetsDetails/manageTargetsDetailsEnabled}"
											items="{/sap.ui5/properties/routing/properties/targets/aTransitionmConvertedEnum}" selectedKey="{ parts:['item'], formatter: '.formatManageTargetsTransition'}" width="100%" tooltip="{i18n>AppDescriptor_Transition_tooltip}">
											<items>
												<core:ListItem key="{key}" text="{text}"></core:ListItem>
											</items>
											<layoutData>
												<layout:GridData span="L6 M6 S6"/>
											</layoutData>
										</DropdownBox>
									</form:fields>
								</form:FormElement>
								<!--Parent-->
								<form:FormElement>
									<form:label>
										<Label id="Parent" text="Parent" wrapping="false">
											<layoutData>
												<layout:GridData linebreak="true" span="L3 M3 S6"/>
											</layoutData>
										</Label>
									</form:label>
									<form:fields>
										<TextField name="targetsParent" change="onRoutingTargetsParentChange" enabled="{/manageTargetsDetails/manageTargetsDetailsEnabled}" value="{parent}" width="100%" tooltip="{i18n>AppDescriptor_Parent_tooltip}">
											<layoutData>
												<layout:GridData span="L6 M6 S6"/>
											</layoutData>
										</TextField>
									</form:fields>
								</form:FormElement>
								<!--Clear Aggregation-->
								<form:FormElement>
									<form:label>
										<Label id="ClearAgg" text="Clear Aggregation" wrapping="false">
											<layoutData>
												<layout:GridData linebreak="true" span="L3 M3 S6"/>
											</layoutData>
										</Label>
									</form:label>
									<form:fields>
										<DropdownBox name="targetsClearAggregation" change="onRoutingTargetsClearAggChange" enabled="{/manageTargetsDetails/manageTargetsDetailsEnabled}"
											items="{/sap.ui5/properties/routing/properties/targets/aBooleanValues}" selectedKey="{ parts:['item'], formatter: '.formatManageTargetsClearAggregation'}" width="100%" tooltip="{i18n>AppDescriptor_Clear_Aggregation_tooltip}">
											<items>
												<core:ListItem key="{key}" text="{text}"></core:ListItem>
											</items>
											<layoutData>
												<layout:GridData span="L6 M6 S6"/>
											</layoutData>
										</DropdownBox>
									</form:fields>
								</form:FormElement>
								<!--Target Parent-->
								<form:FormElement>
									<form:label>
										<Label id="TargetParent" text="Target Parent" wrapping="false">
											<layoutData>
												<layout:GridData linebreak="true" span="L3 M3 S6"/>
											</layoutData>
										</Label>
									</form:label>
									<form:fields>
										<TextField name="targetsTargetParent" change="onRoutingTargetsTargetParentChange" enabled="{/manageTargetsDetails/manageTargetsDetailsEnabled}" value="{targetParent}"
											width="100%" tooltip="{i18n>AppDescriptor_Target_Parent_tooltip}">
											<layoutData>
												<layout:GridData span="L6 M6 S6"/>
											</layoutData>
										</TextField>
									</form:fields>
								</form:FormElement>
							</form:formElements>
						</form:FormContainer>
					</form:formContainers>
					<form:layout>
						<form:ResponsiveGridLayout></form:ResponsiveGridLayout>
					</form:layout>
					<!-- the size of the form in the grid-->
					<form:layoutData>
						<layout:GridData span="L12 M12 S12"/>
					</form:layoutData>
				</form:Form>
			</layout:content>
			<layout:layoutData>
				<layout:GridData span="L12 M12 S12"/>
			</layout:layoutData>
		</layout:Grid>
	</layout:Grid>
