{
  "welcomeFile": "index.html",
  "routes": [
     {
      "path": "/resources/{{projectPath}}",
      "target": {
        "type": "application",
        "name": "{{reuselibrary.libraryName}}"
      },
      "description": "{{reuselibrary.parameters.LibraryDescription.value}}"
    },
    {
      "path": "/test-resources",
      "target": {
        "type": "application",
        "name": "{{reuselibrary.libraryName}}",
        "entryPath": "test"
      },
      "description": "{{reuselibrary.parameters.LibraryDescription.value}}"
    },
    {
      "path": "/resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/resources"
      },
      "description": "SAPUI5 Resources"
    },
    {
      "path": "/test-resources",
      "target": {
        "type": "service",
        "name": "sapui5",
        "entryPath": "/test-resources"
      },
      "description": "SAPUI5 Test Resources"
    }
  ]
}