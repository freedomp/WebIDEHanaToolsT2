{
	"name": "sap.watt.ideplatform.plugin.run.service.DestinationMappingHandler",
	"description": "Destination Mapping section UI Handler interface and Retrieve Backend Systems info",
	"methods": {
		"setServiceData": {
			"params": [
				{
					"name": "oDocument",
					"type": "object"
				},
				{
					"name": "aSourceDestinations",
					"type": "object"
				},
				{
					"name": "aTargetDestinations",
					"type": "object"
				}
			]
		},
		"getDocument": {
			"returns" : {
				"type" : "object",
				"description" : "Selected document"
			}},
		"getSourceDestinations": {
			"returns" : {
				"type" : "object",
				"description" : "List of source destinations"
			}
		},
		"getTargetDestinations": {
			"returns" : {
				"type" : "object",
				"description" : "List of target destinations"
			}
		},
		"getBackendSystemModel": {
			"params": [
				{
					"name": "oModel",
					"type": "object"
				}
			],
			"returns" : {
				"type" : "object",
				"description" : "Backend systems model"
			}

		},
		"onTargetChanged": {
			"params": [
				{
					"name": "oModel",
					"type": "object"
				},{
					"name": "index",
					"type": "number"
				},{
					"name": "sValue",
					"type": "string"
				}				
			]
		},
		"updateModel": {
			"params": [
				{
					"name": "oEvent",
					"type": "object"
				}
			]
		}
	}
}