{
    "name": "sap.watt.hanaplugins.editor.plugin.hdbcalculationview",
    "description": "Calculation View Editor",
    "i18n": "sap.watt.hanaplugins.editor.plugin.hdbcalculationview/i18n/messageBundle.hdbtextbundle",

    "requires": {
        "services": [
            "content",
            "selection",
            "intellisence",
            "editor",
            "document",
            "resource",
            "command",
            "commandGroup",
            "aceeditor",
			"repositorybrowser",
            "focus",
            "calcvieweditor",
            "usernotification",          
            "repositorybrowserUtils",
            "impacteditor",
            "lineageeditor",
            "chebackend.XS2ServicesDAO"
        ]
    },
    "provides": {
        "services": {
            "calcvieweditor": {
                "implements": "sap.watt.hanaplugins.editor.plugin.hdbcalculationview.interface.CalcViewEditor",
                "module": "sap/watt/hanaplugins/editor/plugin/hdbcalculationview/service/CalcViewEditor"
            },
            "impacteditor": {
                "implements": "sap.watt.hanaplugins.editor.plugin.hdbcalculationview.interface.impactEditorContainer",
                "module": "sap/watt/hanaplugins/editor/plugin/hdbcalculationview/impactanalysis/impactEditorContainer"
            },
             "lineageeditor": {
                "implements": "sap.watt.hanaplugins.editor.plugin.hdbcalculationview.interface.lineageEditorContainer",
                "module": "sap/watt/hanaplugins/editor/plugin/hdbcalculationview/impactanalysis/lineageEditorContainer"
            }
                
        },
        "interfaces": {
            "sap.watt.hanaplugins.editor.plugin.hdbcalculationview.interface.CalcViewEditor": "sap/watt/hanaplugins/editor/plugin/hdbcalculationview/service/CalcViewEditor",
            "sap.watt.hanaplugins.editor.plugin.hdbcalculationview.interface.impactEditorContainer": "sap/watt/hanaplugins/editor/plugin/hdbcalculationview/impactanalysis/impactEditorContainer",
            "sap.watt.hanaplugins.editor.plugin.hdbcalculationview.interface.lineageEditorContainer": "sap/watt/hanaplugins/editor/plugin/hdbcalculationview/impactanalysis/lineageEditorContainer"
        }
    },
    "configures": {

        "services": {
            "calcvieweditor:styles": [ {	
                "uri": "sap/watt/hanaplugins/editor/plugin/hdbcalculationview/css/calcvieweditor.css"
            }],
            "impacteditor:styles": [{
                "uri": "sap/watt/hanaplugins/editor/plugin/hdbcalculationview/impactanalysis/impactAnalysisStyle.css"
            },{		
                "uri": "sap/watt/hanaplugins/editor/plugin/hdbcalculationview/css/calcvieweditor.css"		
            }],
            "lineageeditor:styles": [{
                "uri": "sap/watt/hanaplugins/editor/plugin/hdbcalculationview/impactanalysis/impactAnalysisStyle.css"
            }],

            "editor:editors": [{
                "fileExtension": [
                    "hdbcalculationview"
                ],
                "service": "@calcvieweditor",
                "name": "{i18n>tit_calcview_editor}",
                "id": "calcview"
            }, {
                "fileExtension": [
                    "hdbcalculationview"
                ],
                "service": "@aceeditor",
                "name": "{i18n>tit_text_editor}",
                "id": "calcview_ace"
            },{
                "fileExtension": [
                    "impactanalysisview"
                ],
                "service": "@impacteditor",
                "name": "{i18n>tit_ImpactAnalysis_editor }",
                "id": "impactcview"
            },{
                "fileExtension": [
                    "datalineageview"
                ],
                "service": "@lineageeditor",
                "name": "{i18n>tit_DataLineage_editor}",
                "id": "lineagecview"
            }
            ],

            "editor:defaultEditors": [{
                "extention": "hdbcalculationview",
                "editorId": "calcview"
            },{
                "extention": "impactanalysisview",
                "editorId": "impactcview"
            }, {
                "extention": "datalineageview",
                "editorId": "lineagecview"
            }],            

            "command:commands": [{
                "id": "calcvieweditor.run",
                "service": "sap.watt.hanaplugins.editor.plugin.hdbcalculationview/command/Run",
                "label": "{i18n>tit_run_calcview}",
                "icon": "/sap/hana/ide/common/images/lrun_obj.gif",
                "keyBinding": "f8"
            }, {
                "id": "calcvieweditor.create",
                "service": "sap.watt.hanaplugins.editor.plugin.hdbcalculationview/command/NewCalculationViewFile",
                "label": "{i18n>tit_create_new_calcview}"
            }, {
                "id": "calcvieweditor.toggleQuickPane",
                "service": "sap.watt.hanaplugins.editor.plugin.hdbcalculationview/command/ToggleQuickPane",
                "label": "{i18n>tit_toggle_quick_pane}",
                "keyBinding": "mod+q"
            }, {
                "id": "timedatageneration.create",
                "service": "sap.watt.hanaplugins.editor.plugin.hdbcalculationview/command/GenarateTimeDataContributor"
            }, {
                "id": "editor.dyanmic.hdbcalculationview.showPerformanceAnalysis",
                "label": "{i18n>txt_show_hide_performance_analysis}",
                "icon": "sap/hana/ide/editor/plugin/hdbcalculationview/images/performance_workbench.png",
                "type": "action",
                "service": "sap.hana.ide.editor.plugin.hdbcalculationview.command.ShowPerformanceAnalysis"
            },{
                "id": "editor.permanent.hdbcalculationview.calculationViewPreferences",
                "label": "{i18n>txt_calculation_view_preferences}",
                "icon": "sap-icon://settings",
                "type": "action",
                "service": "sap.hana.ide.editor.plugin.hdbcalculationview.command.CalculationViewPreferences"
            },{
                "id": "calcvieweditor.impact",
                "service": "sap.watt.hanaplugins.editor.plugin.hdbcalculationview/command/ImpactAnalysis",
                "label": "Impact Analysis",
                "type": "action"
            }, {
                "id": "calcvieweditor.lineage",
                "service": "sap.watt.hanaplugins.editor.plugin.hdbcalculationview/command/DataLineage",
                "label": "Data Lineage",
                "type": "action"
            }],            

            "commandGroup:items": [{
                "parent": "editor.dynamic",
                "command": "calcvieweditor.run",
                "prio": 70
            }, {
                "parent": "applicationMenu",
                "command": "calcvieweditor.toggleQuickPane",
                "prio": 100
            }, {
                "parent": "file.new",
                "command": "calcvieweditor.create",
                "prio": 4
            }, {
                "parent": "toolbar",
                "type": "menu",
                "icon": "/sap/hana/ide/common/images/stckframe_obj.gif",
                "group": "applicationToolbar",
                "label": "{i18n>txt_menu}",
                "prio": 1
            }, {
                "parent": "editor.permanent.Menu.Tools",
                "command": "timedatageneration.create",
                "label": "{i18n>txt_genarate_time_data}",
                "prio": 5
            }, {
                "parent": "editor.dynamic",
                "command": "editor.dyanmic.hdbcalculationview.showPerformanceAnalysis",
                "prio": 75
            },{
                "parent": "edit.fileActions",
                "command": "calcvieweditor.impact",
                "prio": 10
            }, {
                "parent": "edit.fileActions",
                "command": "calcvieweditor.lineage",
                "prio": 10
            }]
        }
    }
}
