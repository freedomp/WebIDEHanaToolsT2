            "multiple" : false
        },
        "defaultTypes" : {
            "type": "string",
            "multiple" : true
        },
        "dao" : {
			"type" : {
				"service" : "sap.watt.common.service.remote.ProjectTypeDAO"
			},
			"multiple" : false
		}
    },
    "methods" : {
