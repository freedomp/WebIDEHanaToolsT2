{
    "name": "sap.hana.ide.editor.plugin.intellisence",

    "requires": {
        "services": [
            "aceeditor",
            "jscodecompletion"
        ]
    },

    "configures": {

        "services": {
            "intellisence:codeCompletion": [{
                "fileExtension": "xsjs",
                "service": "@jscodecompletion",
                "editor": "aceeditor"
            }]
        }
    },
    "subscribes": {
        
    }
}