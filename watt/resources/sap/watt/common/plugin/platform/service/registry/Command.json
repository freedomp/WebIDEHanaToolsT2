{
	"name" : "sap.watt.common.service.registry.Command",
	"description" : "The command service interface",
	"configurationProperties" : {
		"commands" : {
			"type" : {
				"id" : "string",
				"service" : "sap.watt.common.service.ide.Command",
				"keyBinding" : "string",
				"label" : "string",
				"icon" : "string",
				"iconLabel" : "string",
				"available" : "boolean",
				"enable" : "boolean",
				"windowTarget" : {
					"type" : "string",
					"description" : "setting this property will open a window with given target name, similar to the html anchor tag target attribute"
				}
			},
			"multiple" : true
		}
	},
	"methods" : {
		"getCommand" : {
			"params" : [{
				"name" : "sCommandId",
				"type" : "string"
			}],
			"returns" : "object"
		},
		"filter" : {
			"params" : [{
				"name" : "aItems",
				"type" : "[object]"
			}],
			"returns" : "[object]"
		},
		"invalidateAll" : {
			"description" : "Invalidates all commands and forces them to recalculate their state"
		},
		"registerDocument" : {
			"description" : "register windows document for the keybinding",
			"params" : [{
				"name" : "documentScope",
				"type" : "object"
			}]
		},
		"unregisterDocument" : {
			"description" : "unregister windows document for the keybinding",
			"params" : [{
				"name" : "documentScope",
				"type" : "object"
			}]
		}
	},
	
	"events" : {
		"invalidated" : {
			"params" : [
			   {"name" : "oCommand", "type" : "[ object ]"}
			 ]
		}	
	}
}